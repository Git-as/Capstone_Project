    "from sklearn.model_selection import GridSearchCV\n",
    "from sklearn.tree import DecisionTreeClassifier"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 53,
   "metadata": {},
   "outputs": [],
   "source": [
    "# gridsearch params\n",
    "dtr_params = {\n",
    "    'max_depth': list(range(1, 11))+[None],\n",
    "    'max_features': [None, 1, 2, 3],\n",
    "    'min_samples_split': [2, 3, 4, 5, 10, 15, 20, 25, 30, 40, 50],\n",
    "    'ccp_alpha': [0, 0.001, 0.005, 0.01]\n",
    "}\n",
    "\n",
    "\n",
    "# set the gridsearch\n",
    "model_decision_tree = DecisionTreeClassifier()\n",
    "dtr_gs = GridSearchCV(model_decision_tree, dtr_params, cv=5, verbose=1, n_jobs=-2)"
   ]
  },
